block user, elem icon {
    tag: 'span'
    attrs: {
        return {
            style: [
                'background-image:url(',
                'http://avatars.yandex.net',
                '/get-yapic/',
                this.ctx.uid,
                '/islands-middle',
                '?rnd=',
                (+new Date),
                ')'
            ].join('')
        };
    }
}
