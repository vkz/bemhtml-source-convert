block serp-sitelinks {
    elem info, tag: 'span'
    elem item, tag: 'span'
    elem link, default: {
        var ctx = this.ctx;
        delete ctx.elem;
        applyCtx(this.extend(ctx, {
            block: 'link',
            mix: { block: this.block, elem: this.elem }
        }));
    }

    mod layout table {
        tag: 'table'
        content: { tag: 'tr', content: applyNext() }
        elem cell, tag: 'td'
    }
}
