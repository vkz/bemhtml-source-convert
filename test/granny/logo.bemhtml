block logo {

    !this.elem, content: [
        {
            elem: 'link',
            url: this.ctx.url,
            content: {
                elem: 'image',
                alt: this.ctx.alt,
                url: this.ctx.image
            }
        },
        this.ctx.content
    ]

    elem link {
        tag: 'a'
        attrs: { href: this.ctx.url }
    }

    elem image {
        tag: 'img'
        attrs: {
            alt: this.ctx.alt || '',
            src: this.ctx.url
        }
    }

}
